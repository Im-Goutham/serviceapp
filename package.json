    "emoji-utils": "^1.0.1",
    "haversine": "^1.1.0",
    "mocha": "^5.2.0",
    "native-base": "^2.7.2",
    "npm": "^6.4.1",
    "radio-button-react-native": "^1.1.6",
    "react": "16.4.1",
    "react-native": "0.56.0",
