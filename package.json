{
  "name": "SpotJobs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "cross-os fix-maps"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
